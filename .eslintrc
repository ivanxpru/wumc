{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": false,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "prefer-arrow-callback": 0,
        "func-names": "off",
        "prefer-destructuring": "off",
        "no-console": "off",
        "max-len": "off",
        "no-var": "off",
        "prefer-template": "off",
        "no-alert": "off",
        "operator-assignment": "off"
    }
}